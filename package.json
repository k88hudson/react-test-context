{
  "name": "react-test-context",
  "version": "0.1.0",
  "description": "Utility for testing context",
  "repository": "https://github.com/k88hudson/react-test-context",
  "main": "index.js",
  "scripts": {
    "postversion": "npm publish",
    "test": "npm-run-all test:*",
    "test:legacy": "npm uninstall react-dom react-addons-test-utils && npm install react@^0.13.0 && TEST_ENV=LEGACY karma start",
    "test:latest": "npm install && karma start",
    "start": "npm install && karma start --no-single-run"
  },
  "author": "k88hudson",
  "license": "MPL-2.0",
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "karma": "^0.13.10",
    "karma-babel-preprocessor": "^5.2.2",
    "karma-chrome-launcher": "^0.2.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.3",
    "npm-run-all": "^1.2.11",
    "react": "^0.14.0",
    "react-addons-test-utils": "^0.14.0",
    "react-dom": "^0.14.0",
    "webpack": "^1.12.2"
  }
}
